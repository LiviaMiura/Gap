	<queryString>
		<![CDATA[SELECT
     *,
     documentovinculado.`titulo` AS documentovinculado_titulo,
     documentovinculado.`descricao` AS documentovinculado_descricao,
     documentovinculado.`tipoDocumento_fk` AS documentovinculado_tipoDocumento_fk,
     documentovinculado.`estadoDocumento` AS documentovinculado_estadoDocumento,
     documentovinculado.`data_alteracao` AS documentovinculado_data_alteracao,
     documentovinculado.`template` AS documentovinculado_template,
     documentovinculado.`codigoDocumento` AS documentovinculado_codigoDocumento,
     no.`titulo` AS no_titulo,
     tipodocumento.`tipoDocumento` AS tipodocumento_tipoDocumento,
     no.`descricao` AS no_descricao,
     documentovinculado.`autores` AS documentovinculado_autores,
     documentovinculado.`codigoProduto` AS documentovinculado_codigoProduto,
     documentovinculado.`noPai` AS documentovinculado_noPai,
     tipodocumento.`id_TipoDocumento` AS tipodocumento_id_TipoDocumento,
     itemdono.`id_ItemDoNo` AS itemdono_id_ItemDoNo,
     no.`status` AS no_status
FROM
     `documentovinculado` documentovinculado INNER JOIN `itemdono` itemdono ON documentovinculado.`id_DocumentoVinculado` = itemdono.`documentoVinculado_fk`
     INNER JOIN `no` no ON documentovinculado.`no_fk` = no.`id_No`
     AND no.`id_No` = itemdono.`no_fk`
     INNER JOIN `tipodocumento` tipodocumento ON documentovinculado.`tipoDocumento_fk` = tipodocumento.`id_TipoDocumento`
WHERE
     no.`status` = 1 and  documentovinculado.status = 1
     
ORDER BY
     documentovinculado.`noPai` ASC,
     documentovinculado.`codigoProduto` ASC,
     documentovinculado.`codigoDocumento` ASC,
     tipodocumento.`id_TipoDocumento` ASC]]>
	</queryString>
