	<queryString>
		<![CDATA[SELECT
     documentovinculado.`status` AS documentovinculado_status,
     documentovinculado.`descricao` AS documentovinculado_descricao,
     documentovinculado.`titulo` AS documentovinculado_titulo,
     no.`titulo` AS no_titulo,
     no.`status` AS no_status,
     estadono.`identificacao` AS estadono_identificacao,
     documentovinculado.`no_fk` AS documentovinculado_no_fk,
     no.`id_No` AS no_id_No
FROM
     `documentovinculado` documentovinculado INNER JOIN `itemdono` itemdono ON documentovinculado.`id_DocumentoVinculado` = itemdono.`documentoVinculado_fk`,
     `estadono` estadono INNER JOIN `no` no ON estadono.`id_estadoNo` = no.`estadoNo_fk`
WHERE
     estadono.`id_estadoNo` = $P{a}
 AND no.`status` = 1
     and documentovinculado.`status` = 1
and no.`id_No` = documentovinculado.`no_fk`]]>
	</queryString>
